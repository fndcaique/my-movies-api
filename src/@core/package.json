{
  "name": "core",
  "version": "0.0.1",
  "author": "Fernando Soares <fndcaique@gmail.com>",
  "description": "API for fnd-store",
  "main": "dist/index.js",
  "repository": "https://github.com/fndcaique/fnd-store-api",
  "private": true,
  "keywords": [],
  "imports": {
    "#common/*": "./dist/common/*.js",
    "#category/*": "./dist/category/*.js"
  },
  "typesVersions": {
    ">=5.0": {
      "./category": [
        "./dist/category/index.d.ts"
      ]
    }
  },
  "scripts": {
    "dev": "ts-node-dev --inspect --ignore-watch node_modules --no-notify src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:verbose": "jest --verbose -i",
    "test:cov": "jest --coverage",
    "lint": "eslint -c .eslintrc.json . --ext .ts --ext .js --fix",
    "lint-staged": "lint-staged",
    "build": "tsc -b ./tsconfig.json",
    "build:w": "tsc -b ./tsconfig.json -w",
    "cti": "cti",
    "cti:@core": "sh ./cti.sh"
  },
  "dependencies": {
    "class-validator": "^0.14.0",
    "dotenv": "^16.3.1",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@swc/core": "^1.3.77",
    "@swc/jest": "^0.2.29",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.5.0",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "create-ts-index": "^1.14.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.6.2",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  }
}