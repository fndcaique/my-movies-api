{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "baseUrl": "./",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "sourceMap": true,
    "outDir": "./dist",
    "paths": {
      "#common/*": [
        "./src/common/*"
      ],
      "#category/*": [
        "./src/category/*"
      ]
    }
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules",
    "**/*__tests__/",
    "testing",
    "**/*testing/",
    "**/*.spec.ts"
  ]
}