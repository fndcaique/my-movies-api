{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint",
    "prettier"
  ],
  "rules": {
    "prettier/prettier": 2 // Means error
    // "indent": [
    //   "error",
    //   2
    // ],
    // "linebreak-style": [
    //   "error",
    //   "unix"
    // ],
    // "quotes": [
    //   "error",
    //   "single"
    // ],
    // "semi": [
    //   "error",
    //   "always"
    // ]
  },
  "overrides": [
    {
      "files": [
        "src /**/*spec.ts"
      ],
      "env": {
        "jest": true,
        "node": true
      }
    }
  ]
}